```{r}
#AllMerge <- AllMerge[,c(1:118,124)]
intercept <- lm(HWR~1, data = AllMerge[,-c(2,6,10)])

glm <- lm(HWR~., data = AllMerge[,-c(2,6,10)])
Stepwise <- step(intercept, trace=1, steps = 5, direction = "forward", scope = formula(glm))
summary(Stepwise)
